{
  "appId": "com.github.mt-vite.mt-vite-electron",
  "productName": "MTViteElectron",
  "asar": true,
  "win": {
    "target": {
      "target": "nsis",
      "arch": [
        "x64"
      ]
    }
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  },
  "linux": {
    "executableName": "mt-vite-electron",
    "description": "Muiti target Vite!",
    "desktop": {
      "StartupWMClass": "mt-vite-electron",
      "Keywords": "vite;electron"
    },
    "target": [
      "deb",
      "tar.gz",
      "Appimage"
    ]
  },
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 230,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 230,
        "type": "file"
      }
    ]
  },
  "mac": {
    "target": [
      {
        "target": "dmg",
        "arch": [
          "x64",
          "arm64"
        ]
      }
    ],
    "type": "distribution"
  },
  "files": [
    "dist/**/*",
    "electron/**/*"
  ],
  "directories": {
    "buildResources": "assets",
    "output": "dist_electron"
  }
}